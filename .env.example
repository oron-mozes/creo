# Shared environment variables (available to all agents)
GOOGLE_GENAI_USE_VERTEXAI=0

# Google OAuth Configuration
# Get these from Google Cloud Console > APIs & Services > Credentials
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:8000/api/auth/google/callback

# JWT Secret Key (generate a secure random string for production)
SESSION_SECRET_KEY=your-secret-key-change-in-production

# Creator Finder Agent specific variables
# Variables prefixed with CREATOR_FINDER_AGENT_ will be available without the prefix for this agent
# Example: CREATOR_FINDER_AGENT_GOOGLE_API_KEY will be available as GOOGLE_API_KEY
CREATOR_FINDER_AGENT_GOOGLE_API_KEY=your-api-key-here

# Campaign Brief Agent specific variables
# Variables prefixed with CAMPAING_BRIEF_AGENT_ will be available without the prefix for this agent
CAMPAING_BRIEF_AGENT_GOOGLE_API_KEY=your-api-key-here

# You can also define shared variables without any agent prefix
# These will be available to all agents
# GOOGLE_API_KEY=your-shared-api-key

# Outreach Message Agent specific variables
# Variables prefixed with OUTREACH_MESSAGE_AGENT_ will be available without the prefix for this agent
OUTREACH_MESSAGE_AGENT_GOOGLE_API_KEY=your-api-key-here

# Campaign Builder Agent specific variables
# Variables prefixed with CAMPAIGN_BUILDER_AGENT_ will be available without the prefix for this agent
CAMPAIGN_BUILDER_AGENT_GOOGLE_API_KEY=your-api-key-here


# Frontdesk specific variables
# Variables prefixed with FRONTDESK_ will be available without the prefix for this agent
FRONTDESK_GOOGLE_API_KEY=your-api-key-here


# Onboarding Agent specific variables
# Variables prefixed with ONBOARDING_AGENT_ will be available without the prefix for this agent
ONBOARDING_AGENT_GOOGLE_API_KEY=your-api-key-here


# Suggestions Agent specific variables
# Variables prefixed with SUGGESTIONS_AGENT_ will be available without the prefix for this agent
SUGGESTIONS_AGENT_GOOGLE_API_KEY=your-api-key-here
